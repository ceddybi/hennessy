{
  "name": "promethazine",
  "version": "1.0.0",
  "description": "Kubernetes Automation on DigitalOcean",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "register/",
    "LICENSE"
  ],
  "scripts": {
    "build": "rimraf dist && tsc",
    "do": "mocha src/lib/do/*test.ts --full-trace",
    "k8s": "mocha src/lib/k8s/*test.ts --full-trace"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn eslint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/stoqey/promethazine.git"
  },
  "keywords": [
    "ai",
    "automation",
    "kubernetes",
    "digital",
    "ocean"
  ],
  "author": "Stoqey Inc <support@stoqey.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stoqey/promethazine/issues"
  },
  "homepage": "https://github.com/stoqey/promethazine#readme",
  "dependencies": {
    "@kubernetes/client-node": "^0.12.2",
    "dotenv": "^8.2.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.0.3",
    "chai": "^4.2.0",
    "lodash": "^4.17.20",
    "mocha": "^8.2.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  }
